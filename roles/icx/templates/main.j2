#jinja2: trim_blocks:False
! file generated by {{ username }}
stack disable
!
hostname {{ hostname }}
!
{% if role == 'Router' %}
! router
{% include 'layer3.j2' %}
{% include 'router_acl.j2' %}
{% endif %}
{% else %}
! switch
{% include 'layer2.j2' %}
{% include 'switch_acl.j2' %}
{% endif %}
!
{% include 'aaa.j2' %}
{% include 'time.j2' %}
{% include 'logging.j2' %}
{% include 'snmp.j2' %}
!
!
sz disable
no sz registrar
!
optical-monitor
optical-monitor down-port-enable
optical-monitor non-ruckus-optic-enable
errdisable recovery cause all
errdisable recovery interval 30
console timeout 5
enable acl-per-port-per-vlan
no ip dhcp-client enable
!
{% include 'services.j2' %}
!
{% include 'banner.j2' %}
!
ssh access-group 7
!
!
{% include 'alias.j2' %}
end
